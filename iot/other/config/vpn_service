config general 'global'
	option enable '0'
	option access 'DROP'

config service 'vpn'
	option auth 'SHA256'
	option proto 'udp'
	option port '1194'
	option dev 'tun-SERVER'
	option dev_type 'tun'
	option cipher 'AES-256-GCM'
	option comp 'adaptive'
	option subnet '10.8.0.0'
	option subnetv6 'fd00:db8:0:123::0'
	option mask '255.255.255.0'
	option host '127.0.0.1'
	option start '2'
	option end '100'
	
